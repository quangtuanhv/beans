<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean id="cus1" class= "day4.Customer">
		<property name="name" value="Nguyen Van A"></property>
		<property name="age" value="25"></property>
		<property name="gender" value="Nam"></property>
		<property name="cmnd" value="123456789"></property>
		<property name="codeBooking" ref="cusBooking"></property> 
	</bean>
	<bean id="cusBooking" class= "day4.Booking">
		<property name="codeBooking" value = "211029"></property>
	</bean>
</beans>